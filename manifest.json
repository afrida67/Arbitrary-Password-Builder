{
    "manifest_version": 2,
  
    "name": "Arbitrary Password",
    "description": "Creates random password with all characters and also only with the number",
    "version": "1.0.1",

    "author": "Afrida",
  
    "browser_action": {
     "default_popup": "pass.html",
     "default_icon" : "icon1.png"
   },
    "permissions": [
     "activeTab"
   ],
   "content_scripts": [ {
    "js": [ "pass.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
  
  }
  