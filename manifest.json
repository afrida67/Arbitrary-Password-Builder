{
    "manifest_version": 2,
  
    "name": "Password",
    "description": "Random password generator with all characters and also only number",
    "version": "1.0.1",
  
    "browser_action": {
     "default_popup": "pass.html",
     "default_icon" : "icon1.png"
   },
    "permissions": [
     "activeTab"
   ],
   "content_scripts": [ {
    "js": [ "pass.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
  
  }
  